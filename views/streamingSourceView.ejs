<!DOCTYPE html>
<html>
<head>
  <title>Streaming Sources - Track-Track</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>
</head>
<body>
  <h1>Streaming Sources</h1>
  <a href="/">Back to Home</a>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Edit</th>
        <th>Delete</th>
      </tr>
    </thead>
    <tbody>
      <% streamingSources.forEach(source => { %>
        <tr>
          <td><%= source.name %></td>
          <td><a href="/streamingSource/edit/<%= source._id %>">Edit</a></td>
          <td><a href="/streamingSource/delete/<%= source._id %>" onclick="return confirmDelete();">Delete</a></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
  <a href="/add/addStreamingSource">Add New Streaming Source</a>
</body>
</html>